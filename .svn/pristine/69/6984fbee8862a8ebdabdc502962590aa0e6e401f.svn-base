									<div class="row">
										<div class="col-md-12">
											<button class="btn btn-theme btn-block" type="submit"
												id="addToPreselection">Ajouter à la préselection</button>
										</div>
									</div>
								</div>

							</div>
						</div>
					</div>
				</div>
				<!-- END HIDDEN COMPANY TABLE -->

			</div>
		</div>
	</div>

	<div class="row" id="rowButtons" style="margin-top: 15px;">
